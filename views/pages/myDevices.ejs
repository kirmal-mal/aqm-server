<!DOCTYPE html>
<html>

<head>
  <% include ../partials/header.ejs %>
</head>

<body>
  <form method="GET" action="/mydevices">
    <input type="hidden" id="addDevice" name="adddevice" value="true" />
    <input type="submit" value="add Device" />
  </form>
  <form method="POST" action="/logout">
    <input type="submit" value="Log out" />
  </form>
  <h3>New device token: <%= token %></h3>
  <h2>Devices</h2>
  <ul>
    <% devices.forEach(function(r) { %>
    <li><%= r.id %> - <%= r.model %> - <%= r.serial %> - <%= r.date_connected %> - <a href='<%= "/delete?device_id=" + r.id %>' >X</a></a> - <a href='<%= "/viewlogs?device_id=" + r.id %>' >View logs</a></li>
    <% }); %>
  </ul>
</body>

</html>