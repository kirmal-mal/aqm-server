<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
<div class="container">
<h2>Database Results</h2>

<table border="1">
  <caption>Table Caption</caption>
  <thead>
    <tr>
     <th>ID</th>
     <th>Date</th>
     <th>Environment Data</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach(function(r) { %>
      <tr>
        <td><%= r.id %></td> 
        <td><%= r.date %></td> 
        <td><%= r.env_data %></td>
      </tr>
    <% }); %>
  </tbody>
</table>


</div>

</body>
</html>
